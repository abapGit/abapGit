{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "All Unit Tests",
      "type": "node",
      "request": "launch",
      "pauseForSourceMap": true,
      "trace": false,
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/output/cl_*",
        "${workspaceFolder}/output/index.mjs",
        "${workspaceFolder}/node_modules/@abaplint/runtime/**"
      ],
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/output/index.mjs",
    }
  ]
}
